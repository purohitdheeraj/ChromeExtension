{
    "name": "Clipboard Search",
    "description": "Lets surf the web using our own Clipboard Search extension",
    "version": "0.1",
    "manifest_version": 2 ,
    
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "permissions":[
        "tabs",
        "<all_urls>"
    ]
}